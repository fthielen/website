  shorttitle = {Late Mortality in Survivors of Autologous Hematopoietic-Cell Transplantation},
  author = {Bhatia, Smita and Robison, Leslie L. and Francisco, Liton and Carter, Andrea and Liu, Yan and Grant, Marcia and Baker, K. Scott and Fung, Henry and Gurney, James G. and McGlave, Philip B. and Nademanee, Auayporn and Ramsay, Norma K. C. and Stein, Anthony and Weisdorf, Daniel J. and Forman, Stephen J.},
