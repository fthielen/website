@article{hettle2017,
  title = {The Assessment and Appraisal of Regenerative Medicines and Cell Therapy Products: An Exploration of Methods for Review, Economic Evaluation and Appraisal},
  author = {Hettle, R. and Corbett, M. and Hinde, S. and Hodgson, R. and {Jones-Diette}, J. and Woolacott, N. and Palmer, S.},
  year = {2017},
