@article{wolowacz2016,
  title = {Estimating {{Health}}-{{State Utility}} for {{Economic Models}} in {{Clinical Studies}}: {{An ISPOR Good Research Practices Task Force Report}}},
  author = {Wolowacz, Sorrel E. and Briggs, Andrew and Belozeroff, Vasily and Clarke, Philip and Doward, Lynda and Goeree, Ron and Lloyd, Andrew and Norman, Richard},
