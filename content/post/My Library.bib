  journal = {Health Technol Assess},
  keywords = {Cell- and Tissue-Based Therapy/ economics/ methods,Clinical Trials as Topic/methods/standards,Cost-Benefit Analysis,Humans,Leukemia; B-Cell/therapy,Receptors; Antigen; T-Cell/therapeutic use,Regenerative Medicine/ economics/ methods,Review Literature as Topic,State Medicine,Technology Assessment; Biomedical/ methods,United Kingdom},
