@article{federico2013,
  title = {R-{{CVP}} versus {{R}}-{{CHOP}} versus {{R}}-{{FM}} for the Initial Treatment of Patients with Advanced-Stage Follicular Lymphoma: Results of the {{FOLL05}} Trial Conducted by the {{Fondazione Italiana Linfomi}}},
