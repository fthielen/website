  author = {Ko, R. H. and Ji, L. and Barnette, P. and Bostrom, B. and Hutchinson, R. and Raetz, E. and Seibel, N. L. and Twist, C. J. and Eckroth, E. and Sposto, R. and Gaynon, P. S. and Loh, M. L.},
  year = {2010},
