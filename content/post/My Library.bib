@article{svahn2012,
  title = {Increased Costs after Allogeneic Haematopoietic {{SCT}} Are Associated with Major Complications and Re-Transplantation},
  author = {Svahn, B. M. and Remberger, M. and Alvin, O. and Karlsson, H. and Ringden, O.},
