@article{hatswell2016,
  title = {Regulatory Approval of Pharmaceuticals without a Randomised Controlled Study: Analysis of {{EMA}} and {{FDA}} Approvals 1999\textendash 2014},
  shorttitle = {Regulatory Approval of Pharmaceuticals without a Randomised Controlled Study},
