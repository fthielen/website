@article{kebriaei2009,
  title = {Adult {{Human Mesenchymal Stem Cells Added}} to {{Corticosteroid Therapy}} for the {{Treatment}} of {{Acute Graft}}-versus-{{Host Disease}}},
  author = {Kebriaei, Partow and Isola, Luis and Bahceci, Erkut and Holland, Kent and Rowley, Scott and McGuirk, Joseph and Devetten, Marcel and Jansen, Jan and Herzig, Roger and Schuster, Michael and Monroy, Rod and Uberti, Joseph},
  year = {2009},
  month = jul,
  volume = {15},
