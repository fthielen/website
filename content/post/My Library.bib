@article{wilking2019,
  title = {Achieving Equal and Timely Access to Innovative Anticancer Drugs in the {{European Union}} ({{EU}}): Summary of a Multidisciplinary {{CECOG}}-Driven Roundtable Discussion with a Focus on {{Eastern}} and {{South}}-{{Eastern EU}} Countries},
