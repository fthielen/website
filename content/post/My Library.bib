@article{zhang2018,
  title = {Clopidogrel {{Versus Aspirin}} for the {{Treatment}} of {{Acute Coronary Syndrome After}} a 12-{{Month Dual Antiplatelet Therapy}}: {{A Cost}}-Effectiveness {{Analysis From China Payer}}'s {{Perspective}}},
  shorttitle = {Clopidogrel {{Versus Aspirin}} for the {{Treatment}} of {{Acute Coronary Syndrome After}} a 12-{{Month Dual Antiplatelet Therapy}}},
