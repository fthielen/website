@article{langeveld2003,
  title = {Educational Achievement, Employment and Living Situation in Long-Term Young Adult Survivors of Childhood Cancer in the {{Netherlands}}},
  author = {Langeveld, N. E. and Ubbink, M. C. and Last, B. F. and Grootenhuis, M. A. and Vo{\^U}te, P. A. and de Haan, R. J.},
  year = {2003},
